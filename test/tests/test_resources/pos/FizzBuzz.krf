class A {
  main {
    let current Int = 0
    let max Int = 100
    while(current <= max) {
      if(current % 15 == 0) {
        println("FizzBuzz")
      } else if(current % 3 == 0) {
        println("Fizz")
      } else if(current % 5 == 0) {
        println("Buzz")
      } else {
        println(current)
      }
      max = max + 1
    }
  }
}
